{
  "name": "webdriverio",
  "version": "1.0.0",
  "description": "Task project",
  "main": "index.js",
  "scripts": {
    "run:all-test": "npx wdio run ./wdio.conf.js",
    "run:all-test:headless": "npx wdio run ./wdio.chrome.headless.conf.js",
    "run:logIn-test": "npx wdio run ./wdio.conf.js --spec test/specs/logIn.js",
    "run:SignUp-test": "npx wdio run ./wdio.conf.js --spec test/specs/signUp.js",
    "run:Main-test": "npx wdio run ./wdio.conf.js --spec test/specs/main.js",
    "run:Support-test": "npx wdio run ./wdio.conf.js --spec test/specs/support.js",
    "allure:clean": "rm -rf allure-results/* allure-report/*",
    "allure:serve": "npx allure serve allure-results",
    "allure:generate": "npx allure generate allure-results --clean"
  },
  "keywords": [
    "webdriverIO",
    "docker",
    "allure"
  ],
  "author": "Eduard Maievskyi",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^8.2.0",
    "@wdio/cli": "^8.2.1",
    "@wdio/local-runner": "^8.2.1",
    "@wdio/mocha-framework": "^8.2.1",
    "@wdio/spec-reporter": "^8.1.2",
    "chromedriver": "^109.0.0",
    "wdio-chromedriver-service": "^8.0.1"
  },
  "dependencies": {
    "allure-commandline": "^2.20.1"
  }
}
